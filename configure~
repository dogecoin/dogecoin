else case e in #(
  e)
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
    if test x$use_reduce_exports = xyes; then
      as_fn_error $? "Cannot find a working visibility attribute. Use --disable-reduce-exports." "$LINENO" 5
    fi

 ;;
esac
fi
