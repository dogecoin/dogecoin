  COMPAT_LDFLAGS="$COMPAT_LDFLAGS -Wl,-no-as-needed -Wl,-lrt -Wl,-as-needed"
else case e in #(
  e) : ;;
esac
fi


  #__fdelt_chk's params and return type have changed from long unsigned int to long int.
  # See which one is present here.
