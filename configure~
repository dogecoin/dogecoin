{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for Linux getrandom syscall" >&5
printf %s "checking for Linux getrandom syscall... " >&6; }
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <unistd.h>
  #include <sys/syscall.h>
  #include <linux/random.h>
