then :
  CXXFLAGS="$CXXFLAGS -Wno-unused-parameter"
else case e in #(
  e) : ;;
esac
fi

  as_CACHEVAR=`printf "%s\n" "ax_cv_check_cxxflags_$CXXFLAG_WERROR_-Wself-assign" | sed "$as_sed_sh"`
