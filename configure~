  enableval=$enable_experimental; allow_experimental=$enableval
else case e in #(
  e) allow_experimental=no ;;
esac
fi


# Check whether --enable-scrypt-sse2 was given.
if test ${enable_scrypt_sse2+y}
then :
  enableval=$enable_scrypt_sse2; use_scrypt_sse2=$enableval
else case e in #(
  e) use_scrypt_sse2=no ;;
esac
fi



# Check whether --with-intel-avx2 was given.
if test ${with_intel_avx2+y}
then :
  withval=$with_intel_avx2; intel_avx2=$withval
else case e in #(
  e) intel_avx2=no ;;
esac
fi



# Check whether --with-armv8-crypto was given.
if test ${with_armv8_crypto+y}
then :
  withval=$with_armv8_crypto; armv8_crypto=$withval
else case e in #(
  e) armv8_crypto=no ;;
esac
fi



# Check whether --with-armv82-crypto was given.
if test ${with_armv82_crypto+y}
then :
  withval=$with_armv82_crypto; armv82_crypto=$withval
else case e in #(
  e) armv82_crypto=no ;;
esac
fi


# Check whether --enable-man was given.
if test ${enable_man+y}
then :
  enableval=$enable_man;
else case e in #(
  e) enable_man=yes ;;
esac
fi

 if test "$enable_man" != no; then
  ENABLE_MAN_TRUE=
  ENABLE_MAN_FALSE='#'
else
  ENABLE_MAN_TRUE='#'
  ENABLE_MAN_FALSE=
fi


# Enable debug
# Check whether --enable-debug was given.
if test ${enable_debug+y}
then :
  enableval=$enable_debug; enable_debug=$enableval
else case e in #(
  e) enable_debug=no ;;
esac
fi


# Turn warnings into errors
# Check whether --enable-werror was given.
if test ${enable_werror+y}
then :
  enableval=$enable_werror; enable_werror=$enableval
else case e in #(
  e) enable_werror=no ;;
esac
fi


ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether C++ compiler accepts -Werror" >&5
printf %s "checking whether C++ compiler accepts -Werror... " >&6; }
if test ${ax_cv_check_cxxflags___Werror+y}
then :
  printf %s "(cached) " >&6
else case e in #(
  e)
  ax_check_save_flags=$CXXFLAGS
  CXXFLAGS="$CXXFLAGS  -Werror"
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main (void)
{

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_compile "$LINENO"
then :
  ax_cv_check_cxxflags___Werror=yes
else case e in #(
  e) ax_cv_check_cxxflags___Werror=no ;;
esac
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
  CXXFLAGS=$ax_check_save_flags ;;
esac
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ax_cv_check_cxxflags___Werror" >&5
printf "%s\n" "$ax_cv_check_cxxflags___Werror" >&6; }
if test "x$ax_cv_check_cxxflags___Werror" = xyes
then :
  CXXFLAG_WERROR="-Werror"
else case e in #(
  e) CXXFLAG_WERROR="" ;;
esac
fi


if test "x$enable_debug" = xyes; then
    CPPFLAGS="$CPPFLAGS -DDEBUG -DDEBUG_LOCKORDER"
    if test "x$GCC" = xyes; then
        CFLAGS="$CFLAGS -g3 -O0"
    fi

    if test "x$GXX" = xyes; then
        CXXFLAGS="$CXXFLAGS -g3 -O0"
    fi
fi

ERROR_CXXFLAGS=
if test "x$enable_werror" = "xyes"; then
  if test "x$CXXFLAG_WERROR" = "x"; then
    as_fn_error $? "\"enable-werror set but -Werror is not usable\"" "$LINENO" 5
  fi
  as_CACHEVAR=`printf "%s\n" "ax_cv_check_cxxflags_$CXXFLAG_WERROR_-Werror=vla" | sed "$as_sed_sh"`
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether C++ compiler accepts -Werror=vla" >&5
printf %s "checking whether C++ compiler accepts -Werror=vla... " >&6; }
if eval test \${$as_CACHEVAR+y}
then :
  printf %s "(cached) " >&6
else case e in #(
  e)
  ax_check_save_flags=$CXXFLAGS
  CXXFLAGS="$CXXFLAGS $CXXFLAG_WERROR -Werror=vla"
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main (void)
{

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_compile "$LINENO"
then :
  eval "$as_CACHEVAR=yes"
else case e in #(
  e) eval "$as_CACHEVAR=no" ;;
esac
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
  CXXFLAGS=$ax_check_save_flags ;;
esac
fi
eval ac_res=\$$as_CACHEVAR
	       { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5
printf "%s\n" "$ac_res" >&6; }
if eval test \"x\$"$as_CACHEVAR"\" = x"yes"
then :
  ERROR_CXXFLAGS="$ERROR_CXXFLAGS -Werror=vla"
else case e in #(
  e) : ;;
esac
fi

fi

if test "x$CXXFLAGS_overridden" = "xno"; then
  as_CACHEVAR=`printf "%s\n" "ax_cv_check_cxxflags_$CXXFLAG_WERROR_-Wall" | sed "$as_sed_sh"`
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether C++ compiler accepts -Wall" >&5
