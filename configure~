{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_objcxx_compiler_gnu" >&5
printf "%s\n" "$ac_cv_objcxx_compiler_gnu" >&6; }
ac_compiler_gnu=$ac_cv_objcxx_compiler_gnu

