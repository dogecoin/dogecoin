      fi

  else
    true

  fi


  if test "x$bitcoin_enable_qt" != "xno" && test x$bitcoin_qt_want_version != xno; then
    true


pkg_failed=no
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for ${QT_LIB_PREFIX}Test" >&5
printf %s "checking for ${QT_LIB_PREFIX}Test... " >&6; }

